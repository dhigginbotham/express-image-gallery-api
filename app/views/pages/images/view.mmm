{{> parts/nav/nav}}

<div class="container">
  <legend>
    View Images
  </legend>
  <table class="table table-bordered table-striped">
    <tbody>
  {{#images}}
      <tr>
        <td>
          <a href="/images/{{_id}}/edit"><img src="/uploads/{{unique}}" alt="{{title}}" /></a>
        </td>
      </tr>
  {{/images}}
    </tbody>
  </table>
  <br>
  {{#images.pagination}}
    {{> parts/paging}}
  {{/images.pagination}}

</div>
